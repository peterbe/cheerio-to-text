{
  "name": "cheerio-to-text",
  "version": "0.2.4",
  "description": "Turn a Cheerio object into plain text",
  "repository": {
    "type": "git",
    "url": "git://github.com/peterbe/cheerio-to-text.git"
  },
  "bugs": {
    "url": "https://github.com/peterbe/cheerio-to-text/issues"
  },
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "module": "lib/esm/index.js",
  "files": [
    "lib"
  ],
  "exports": {
    "require": "./lib/index.js",
    "import": "./lib/esm/index.js"
  },
  "scripts": {
    "test": "npm run test:test && npm run lint",
    "test:test": "node --import tsx --test test/*.ts",
    "lint": "npm run lint:es && npm run lint:prettier",
    "lint:es": "eslint src",
    "lint:prettier": "npm run prettier -- --check",
    "format": "npm run format:es && npm run format:prettier",
    "format:es": "npm run lint:es -- --fix",
    "format:prettier": "npm run prettier -- --write",
    "prettier": "prettier '**/*.{ts,md,json,yml}'",
    "build:watch": "tsc --watch",
    "build": "npm run build:cjs && npm run build:esm",
    "build:cjs": "tsc",
    "build:esm": "npm run build:cjs -- --module esnext --target es2019 --outDir lib/esm && echo '{\"type\":\"module\"}' > lib/esm/package.json",
    "prepare": "npm run build",
    "release": "npm run prepare && np"
  },
  "keywords": [
    "cheerio",
    "html",
    "text"
  ],
  "author": "Peter Bengtsson",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "22.1.0",
    "@typescript-eslint/eslint-plugin": "7.15.0",
    "@typescript-eslint/parser": "7.18.0",
    "eslint": "8.57.1",
    "eslint-config-prettier": "9.1.0",
    "np": "10.0.7",
    "prettier": "3.3.3",
    "tsx": "4.19.1",
    "typescript": "5.5.4"
  },
  "peerDependencies": {
    "cheerio": "1.0.0-rc.12"
  },
  "prettier": {
    "semi": false
  },
  "engines": {
    "node": ">=18"
  }
}
